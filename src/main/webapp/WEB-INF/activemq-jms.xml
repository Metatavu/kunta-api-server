<?xml version="1.0" encoding="UTF-8"?>
<messaging-deployment xmlns="urn:jboss:messaging-activemq-deployment:1.0">
  <server name="default">
    <jms-destinations>
      <jms-queue name="kunta-rekty-jobs">
        <entry name="java:/jms/queue/kunta-rekry-jobs"/>
        <durable>true</durable>
      </jms-queue>
      <jms-queue name="ptv-service-channels">
        <entry name="java:/jms/queue/ptv-service-channels"/>
        <durable>true</durable>
      </jms-queue>
      <jms-queue name="ptv-services">
        <entry name="java:/jms/queue/ptv-services"/>
        <durable>true</durable>
      </jms-queue>
    </jms-destinations>
  </server>
</messaging-deployment>

