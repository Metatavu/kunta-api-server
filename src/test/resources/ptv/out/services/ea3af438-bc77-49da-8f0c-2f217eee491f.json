  "serviceDescriptions" : [ {
    "language" : "fi",
    "value" : "Vanhemmat tai oppilas itse voi ottaa yhteyttä oman koulunsa kouluterveydenhoitajaan, koulupsykologiin tai koulukuraattoriin joko puhelimitse, sähköpostilla tai Wilma-viestillä.",
    "type" : "UserInstruction"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ChargeTypeAdditionalInfo"
  }, {
    "language" : "fi",
