  "serviceDescriptions": [
    {
      "language": "fi",
      "value": "Lapselle on mahdollista saada tilapäinen hoitopaikka väliaikaiseen tarpeeseen.",
      "type": "Summary"
    },
    {
      "language": "fi",
      "value": null,
      "type": "DeadLine"
    },
    {
      "language": "fi",
