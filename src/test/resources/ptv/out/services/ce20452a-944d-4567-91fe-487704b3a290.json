  "serviceDescriptions" : [ {
    "language" : "sv",
    "value" : "Myndighetsverksamheten och dess handlingarna är i regel offentlig.",
    "type" : "Summary"
  }, {
    "language" : "sv",
    "value" : null,
    "type" : "ProcessingTime"
  }, {
    "language" : "fi",
