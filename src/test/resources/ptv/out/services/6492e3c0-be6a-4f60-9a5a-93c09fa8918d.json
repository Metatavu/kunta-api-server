  "serviceDescriptions" : [ {
    "language" : "fi",
    "value" : "Vaasan Ruokapalvelut valmistaa Vaasan kaupungin päiväkotien ja vuorohoitopäiväkodin ruoan.",
    "type" : "Summary"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "DeadLine"
  }, {
    "language" : "fi",
