  "serviceDescriptions" : [ {
    "language" : "sv",
    "value" : "Detaljhandel med tobakprodukter och nikotinvätskor förutsätter ett tillstånd.",
    "type" : "Summary"
  }, {
    "language" : "sv",
    "value" : "Den beräknade behandlingstiden är 1-4 veckor.",
    "type" : "ProcessingTime"
  }, {
    "language" : "fi",
    "value" : "Arvioitu käsittelyaika on 1-4 viikkoa.",
    "type" : "ProcessingTime"
  }, {
    "language" : "fi",
    "value" : "Määräaikainen vähittäismyyntilupa voidaan myöntää enintään vuodeksi. Myyjän tulee ilmoittaa tukkumyynnin lopettamisesta.",
    "type" : "ValidityTime"
  }, {
    "language" : "sv",
    "value" : "Ett riksomfattade register över försäljningsplatserna för nikotinprodukter upprätthålls av tillstånds- och tillsynsverket för social- och hälsovården (Valvira). ",
    "type" : "Description"
  }, {
    "language" : "sv",
    "value" : null,
    "type" : "DeadLine"
  }, {
    "language" : "sv",
    "value" : "Ett detaljhandelstillstånd på viss tid kan beviljas för högst ett år. Försäljaren ska meddela om avslutande av partiförsäljning.",
    "type" : "ValidityTime"
  }, {
    "language" : "sv",
    "value" : "Avgiften för detaljhandelstillståndet för tobaksprodukter och nikotinpreparat och den årliga tillsynsavgiften gällande försäljningen fastställs enligt avgiftstaxan för miljö- och hälsoskydd inom Vasa stads och Laihela kommuns samarbetsområde.",
    "type" : "ChargeTypeAdditionalInfo"
  }, {
    "language" : "sv",
    "value" : null,
    "type" : "UserInstruction"
  }, {
    "language" : "fi",
    "value" : "Tupakkatuotteiden ja nikotiininesteiden vähittäismyynti edellyttää luvan.",
    "type" : "Summary"
  }, {
    "language" : "fi",
    "value" : "Valtakunnallista rekisteriä nikotiinivalmisteiden myyntipaikoista ylläpitää sosiaali- ja terveysalan lupa- ja valvontavirasto (Valvira). ",
    "type" : "Description"
  }, {
    "language" : "fi",
    "value" : "Ohjeet lupahakemuksen ja omavalvontasuunnitelman tekemiseen sekä myyntilupahakemus löytyvät Valviran verkkosivuilta. ",
    "type" : "UserInstruction"
  }, {
    "language" : "fi",
    "value" : "Tupakkatuotteiden ja nikotiinivalmisteiden vähittäismyyntilupamaksu ja myynnin vuosittainen valvontamaksu määräytyy Vaasan kaupungin ja Laihian kunnan yhteistoiminta-alueen ympäristöterveydenhuollon maksutaksan mukaisesti.",
    "type" : "ChargeTypeAdditionalInfo"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "DeadLine"
  } ],
