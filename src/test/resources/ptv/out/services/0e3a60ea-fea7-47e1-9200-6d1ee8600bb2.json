  "serviceDescriptions" : [ {
    "language" : "fi",
    "value" : "Avoimen varhaiskasvatuksen palveluja Naantalissa ovat Väentuvan avoin päiväkoti ja kerhotoiminta Pikkutalon Viskari-kerhoryhmissä.",
    "type" : "Summary"
  }, {
    "language" : "fi",
    "value" : "Avoimen varhaiskasvatuksen palveluja Naantalissa ovat Väentuvan avoin päiväkoti ja 3 - 5 -vuotiaiden kerhotoiminta Pikkutalon kahdessa Viskari-kerhoryhmässä.  \n* Avoin päiväkoti on yhteinen tapaamis- ja toimintapaikka naantalilaisille  vauva- ja lapsiperheille, joiden lapset ovat kotihoidossa.\n* Kerhotoiminnassa leikitään ja puuhastellaan yhdessä kolme kertaa viikossa. Kerhossa lapset totuttelevat olemaan muutaman tunnin ajan erossa vanhemmista vieraassa paikassa rakentaen luottamuksellista suhdetta muihin aikuisiin ja lapsiin.\n",
    "type" : "Description"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ValidityTime"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ProcessingTime"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "DeadLine"
  }, {
    "language" : "fi",
    "value" : "Avoin päiväkoti on maksuton. Viskari-kerhoryhmien kuukausimaksu on 30 euroa (poissaoloja ei vähennetä).",
    "type" : "ChargeTypeAdditionalInfo"
  }, {
    "language" : "fi",
    "value" : "Maksuttomaan avoimeen päiväkotiin ei tarvitse ilmoittautua tai hakea. \n\nMaksulliseen kerhotoimintaan (Viskari-kerhoryhmät) haetaan ilmoittautumislomakkeella. Kerhoon voi hakea kun lapsi on täyttänyt 3 vuotta. Toimintavuoden aikana mahdollisesti vapautuvat paikat täytetään hakujärjestyksessä.",
    "type" : "UserInstruction"
  } ],
