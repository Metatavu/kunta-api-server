  "serviceDescriptions" : [ {
    "language" : "fi",
    "value" : "Esiopetusikäinen saa päivähoitoa samassa paikassa kuin esiopetusta. Naantali tarjoaa esiopetusta päiväkodeissa tai kouluissa.",
    "type" : "Summary"
  }, {
    "language" : "fi",
    "value" : "Jos lapsi esiopetuksen aloittaessaan ei ole päivähoidossa ja tarvitsee esiopetuksen jälkeen tai sitä ennen päivähoitoa, tulee siitä tehdä erillinen päivähoitohakemus (varhaiskasvatushakemus) verkkopalvelussa.",
    "type" : "UserInstruction"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ChargeTypeAdditionalInfo"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "DeadLine"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ProcessingTime"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ValidityTime"
  }, {
    "language" : "fi",
    "value" : "Naantalissa saa esiopetusta päiväkodeissa tai kouluissa. Jos lapsi tarvitsee päivähoitoa esiopetuksen jälkeen tai ennen sitä, hän saa sitä samassa paikassa kuin esiopetusta, ja sisällöltään se on tavallista päiväkotitoimintaa.\n\nVaihtoehdot esioppilaan päivähoidolle ovat enintään 20 tuntia/viikko tai 20 - 35 tuntia/viikko.",
    "type" : "Description"
  } ],
