  "serviceDescriptions" : [ {
    "language" : "fi",
    "value" : "Rakennusta tai sen osaa ei saa ilman lupaa purkaa asemakaava-alueella tai rakennuskieltoalueella (yleiskaava voi myös määrätä luvan haettavaksi).\n  \nPurkuilmoitus tehdään, kun kyseessä on pienehkö alle 30 m2 rakennus. Se tulee toimittaa rakennusvalvontatoimistoon, jossa ilmoitus käsitellään ja siitä tehdään myös päätös.",
    "type" : "Description"
  }, {
    "language" : "fi",
    "value" : "Naantalin kaupungin rakennusvalvonnan taksat löydät Naantalin kaupungin kotisivuilta.",
    "type" : "ChargeTypeAdditionalInfo"
  }, {
    "language" : "fi",
    "value" : "Purkamistoimenpiteen päättyminen - rakennusvalvonnan loppukatselmus suoritettu (purkamisluvat) - ilmoitus purkamistyön suorittamisesta tehty rakennusvalvontaan (purkamisilmoitukset)",
    "type" : "ValidityTime"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ProcessingTime"
  }, {
    "language" : "fi",
    "value" : "Kirjallinen ilmoitus on tehtävä rakennusvalvontaan vähintään 30 pv ennen purkamistyön suunniteltua aloittamista.",
    "type" : "DeadLine"
  }, {
    "language" : "fi",
    "value" : "Naantalissa rakennusluvat haetaan ensisijaisesti Lupapiste.fi:n kautta. ",
    "type" : "UserInstruction"
  }, {
    "language" : "fi",
    "value" : "Rakennusta tai sen osaa ei saa ilman lupaa purkaa asemakaava-alueella tai rakennuskieltoalueella (yleiskaava voi myös määrätä luvan haettavaksi).",
    "type" : "Summary"
  } ],
