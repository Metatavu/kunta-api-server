  "serviceDescriptions" : [ {
    "language" : "fi",
    "value" : "Yläkouluikäisille suunnatun valmennustoiminnan toimintamuotoja ovat lajivalmennus 8-9lk, urheiluyläkoulu sekä yläkoululeiritysmalli.",
    "type" : "Summary"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ProcessingTime"
  }, {
    "language" : "fi",
    "value" : null,
    "type" : "ValidityTime"
