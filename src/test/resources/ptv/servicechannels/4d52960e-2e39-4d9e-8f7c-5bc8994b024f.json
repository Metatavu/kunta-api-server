  "serviceChannelDescriptions" : [ {
    "language" : "fi",
    "value" : "Ihastjärven koulu on neliopettajainen, reilun 50 oppilaan alakoulu, jossa toimii luokat 0-6.",
    "type" : "ShortDescription"
  }, {
    "language" : "fi",
    "value" : "Ihastjärven koulu sijaitsee noin 20 kilometrin päässä Mikkelin keskustasta. Uusi Ihastjärven koulu on valmistunut vuonna 2012. Koulun tilat on suunniteltu yhdessä oppilaiden kanssa. Neliopettajaisessa koulussa annetaan yhdysluokkaopetusta ryhmissä 0-2 -luokat, 3-4 -luokat ja 5-6 -luokat. Esioppilaat ovat samassa ryhmässä 1-2 -luokkalaisten kanssa",
    "type" : "Description"
  } ],
